{
    "name": "portfolio2",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts-ts": "^4.0.8"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o build/  -r -i --include-path --output-style expanded",
        "watch-css": "npm run build-css && node-sass-chokidar src/styles/ -o src/styles/  -w -r",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts-ts build",
        "build-dev": "npm run build-css && react-scripts-ts build",
        "start-js": "react-scripts-ts start",
        "eject": "react-scripts-ts eject"
    },
    "devDependencies": {
        "@types/jest": "^23.3.10",
        "@types/node": "^10.12.11",
        "@types/react": "^16.7.11",
        "@types/react-dom": "^16.0.11",
        "@types/react-router": "^4.4.1",
        "@types/react-router-dom": "^4.3.1",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "typescript": "^3.2.1"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
